<p id="notice"><%= notice %></p>

<h1>Listing Commoditycategories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @commoditycategories.each do |commoditycategory| %>
      <tr>
        <td><%= commoditycategory.name %></td>
        <td><%= link_to 'Show', commoditycategory %></td>
        <td><%= link_to 'Edit', edit_commoditycategory_path(commoditycategory) %></td>
        <td><%= link_to 'Destroy', commoditycategory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Commoditycategory', new_commoditycategory_path %>
